/* 公共样式 */

.clearfix:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}

.clearfix {
    zoom: 1;
}

.ovh {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ovh2 {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: initial;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.scroll-view-x {
    white-space: nowrap;
}

.scroll-view-y {
    height: 100%;
}

/* 搜索 */

.search-wrapper {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    padding: 15rpx 4%;
    z-index: 5;
    display: flex;
}

.search-wrapper .inp-wrapper {
    flex: 1;
    padding: 0 20rpx;
    background-color: rgba(255, 255, 255, 0.5);
    border-radius: 30rpx;
}

.search-wrapper .inp {
    height: 50rpx;
    text-align: center;
    color: #666;
    font-size: 26rpx;
}

.search-wrapper .holder-inp {
    text-align: center;
    color: #999;
    font-size: 26rpx;
    background-color: transparent;
}
.search-wrapper.act{
    background-color: #f05077;
}
.search-wrapper.act .inp-wrapper{
    background-color: #fff;
}

/* 分割线模板样式 */

.split {
    height: 20rpx;
    background-color: #f7f7f7;
}

/* 分类标题模板样式 */

.product-title {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 25rpx 0;
    text-align: center;
    line-height: 40rpx;
}

.product-title .icon {
    flex: 1;
    height: 80rpx;
    line-height: 80rpx;
}

.product-title .text {
    width: 300rpx;
    font-size: 30rpx;
}

.product-title .detail {
    font-size: 20rpx;
    color: #999;
}

.product-title .icon-left {
    width: 50rpx;
    height: 39rpx;
}

.product-title .icon-right {
    width: 50rpx;
    height: 19rpx;
}

/* 首页产品item样式 */

.product-wrapper {
    padding: 0 30rpx;
}

.product-item {
    display: inline-block;
    width: 200rpx;
    margin-right: 20rpx;
}

.product-item:last-child {
    margin-right: 0;
}

.product-item .img {
    width: 200rpx;
    height: 200rpx;
    vertical-align: top;
}

.product-item .title {
    height: 52rpx;
    margin-top: 10rpx;
    font-size: 20rpx;
    color: #333;
    line-height: 26rpx;
}

.product-item .price {
    margin-top: 5rpx;
    font-size: 22rpx;
    color: #f05077;
}

.product-item .discount {
    font-size: 22rpx;
    line-height: 36rpx;
    color: #f05077;
}

.product-item .yhq-icon {
    width: 36rpx;
    height: 36rpx;
    vertical-align: top;
}

/* 产品的另一种布局 */

.goods-item-wrapper {
    position: relative;
    border-bottom: 1rpx solid #ccc;
}

.goods-item {
    display: flex;
    padding: 30rpx;
}

.goods-item-wrapper:last-child {
    border-bottom: none;
}

.goods-item .img {
    width: 200rpx;
    height: 200rpx;
    margin-right: 20rpx;
}

.goods-item .goods-info {
    flex: 1;
}

.goods-item .goods-info .title {
    font-size: 26rpx;
    line-height: 36rpx;
    max-height: 72rpx;
    margin-bottom: 15rpx;
}

.goods-item .goods-info .price {
    float: left;
    font-size: 40rpx;
    color: #f05077;
    line-height: 50rpx;
}

.goods-item .goods-info .num {
    float: right;
    font-size: 22rpx;
    color: #999;
    line-height: 50rpx;
}

.goods-item .goods-info .old-price {
    font-size: 22rpx;
    margin-left: 15rpx;
    color: #999;
    text-decoration: line-through;
}

.goods-item .goods-info .other {
    margin-top: 15rpx;
    line-height: 40rpx;
}

.goods-item .goods-info .coupon {
    float: left;
    font-size: 26rpx;
    color: #f05077;
}

.goods-item .goods-info .coupon-icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 6rpx;
    margin-top: 2rpx;
    vertical-align: top;
}

.goods-item .goods-info .shopname {
    float: right;
    font-size: 24rpx;
    color: #999;
}

.goods-item .goods-info .icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 6rpx;
    margin-top: 5rpx;
    vertical-align: top;
}

.goods-item .exchange-sroce {
    font-size: 30rpx;
    color: #f05077;
    line-height: 60rpx;
    padding-top: 10rpx;
    margin-bottom: 10rpx;
}

.goods-item .jf-icon {
    width: 48rpx;
    height: 48rpx;
    margin-right: 15rpx;
    margin-top: 6rpx;
    vertical-align: top;
}
.goods-item .order-num{
    float: right;
    color: #999;
    font-size: 24rpx;
}
.goods-item .exchange-date{
    font-size: 24rpx;
    color: #999;
}
.exchange-btn {
    position: absolute;
    right: 30rpx;
    bottom: 77rpx;
    padding: 0 20rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    color: #999;
    border-radius: 10rpx;
    background-color: #f7f7f7;
    border: 1px solid #ccc;
}

/* 选择 */

.isCheck {
    vertical-align: top;
    margin-top: 19rpx;
    width: 32rpx;
    height: 32rpx;
    margin-right: 15rpx;
}

/* 分类 */

.type-classify {
    border-bottom: 1rpx solid #ccc;
}

.type-classify .item {
    display: inline-block;
    font-size: 28rpx;
    line-height: 40rpx;
    color: #999;
    padding: 20rpx 25rpx;
}

.type-classify .item.act {
    color: #f05077;
}

.sort-wrapper {
    display: flex;
    align-items: center;
    text-align: center;
    border-bottom: 1rpx solid #ccc;
}

.sort-wrapper .item {
    flex: 1;
    color: #999;
    font-size: 28rpx;
    padding: 20rpx 0;
    line-height: 40rpx;
}

.sort-wrapper .item.act {
    color: #f05077;
}

.sort-wrapper .coupon {
    font-size: 28rpx;
    width: 36%;
    color: #999;
    line-height: 70rpx;
    text-align: right;
    padding-right: 30rpx;
}

/* 分类固定 */

.fixed-classify {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
}

.top-one {
    padding-top: 81rpx;
    box-sizing: border-box;
    height: 100%;
}

.top-two {
    padding-top: 162rpx;
    box-sizing: border-box;
    height: 100%;
}

/* 到底了公共样式 */

.no-data, .loadding-data {
    text-align: center;
    padding: 25rpx 0;
    background-color: #f7f7f7;
    font-size: 24rpx;
    color: #999;
}

/* 查看更多 */

.product-list .see-more-btn {
    padding: 20rpx 0;
    text-align: center;
    font-size: 22rpx;
    color: #999;
}

.loadding-more {
    padding: 25rpx 0;
    font-size: 24rpx;
    color: #fff;
    text-align: center;
}

/* 我的详情列表样式 */

.common-item {
    padding: 20rpx 30rpx;
    border-bottom: 1px solid #ebebeb;
}

.common-item .avatar {
    width: 56rpx;
    height: 56rpx;
    margin-right: 15rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.common-item .avatar image {
    width: 100%;
    height: 100%;
}

.common-item .item {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    line-height: 40rpx;
    padding: 10rpx 0;
}

.common-item .item .num {
    width: 160rpx;
    text-align: right;
}

.common-item .item .title {
    flex: 1;
}

/* 我的类别样式 */

.home-item {
    padding: 30rpx 0;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
}

.home-item .item {
    flex: 1;
    font-size: 26rpx;
    color: #666;
}

.home-item .item .num {
    margin-bottom: 10rpx;
    font-size: 34rpx;
    color: #f05077;
}

/* 切换样式 */

.line-tab {
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    height: 79rpx;
    border-bottom: 1rpx solid #ebebeb;
}
.push-wrapper.act{
    padding-top: 80rpx;
}
.push-wrapper.act .line-tab{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    background-color: #fff;
    z-index: 15;
}

.line-tab .item {
    flex: 1;
    padding: 20rpx 0 18rpx;
    font-size: 24rpx;
    line-height: 40rpx;
    color: #666;
}

.line-tab .item.act {
    color: #f05077;
    border-bottom: 2rpx solid #f05077;
}
